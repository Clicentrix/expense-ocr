MYSQL_HOST=localhost
MYSQL_USER=your_mysql_user
MYSQL_PASSWORD=your_mysql_password
MYSQL_DB=expense_management

# Google Cloud Vision API credentials (for OCR)
# You'll need to set up a service account and point to the JSON key file
GOOGLE_APPLICATION_CREDENTIALS="/path/to/your/google-cloud-service-account-key.json"

# Gemini API Key (from Google AI Studio or GCP Console)
GEMINI_API_KEY="YOUR_GEMINI_API_KEY_HERE"

# Commenting out Document AI specific config as we shift to Gemini for parsing
# GCP_PROJECT_ID="your-gcp-project-id"
# DOCAI_LOCATION="us" 
# DOCAI_PROCESSOR_ID="your-docai-processor-id"

FLASK_APP=app.py
FLASK_ENV=development
PORT=5000 